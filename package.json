{
  "name": "@sspiff/handyaws",
  "author": "sspiff",
  "license": "ISC",
  "version": "0.1.2",
  "description": "Handy bits of javascript for AWS",
  "main": "index.js",
  "files": [
    "index.js",
    "client.js",
    "parseArn.js",
    "getTagsFor/*",
    "keyValue/*",
    "lambda/*",
    "lambdaEdge/*"
  ],
  "sideEffects": false,
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(@sspiff/handy)/)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/__tests__/jest.setup.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "jest.setup.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sspiff/handyaws-js.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "bugs": {
    "url": "https://github.com/sspiff/handyaws-js/issues"
  },
  "homepage": "https://github.com/sspiff/handyaws-js#readme",
  "dependencies": {
    "@sspiff/handy": "^0.1.1",
    "fast-memoize": "^2.5.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.8.4",
    "jest": "^27.5.1"
  }
}
